    <Container style='bottom:0px;right:0px;'> 
      <PushButton name='ShowMemInfo' text='Refresh'        onclick='function(event)
        {
          var ev = new TEvent ( event ) ;
          var w = ev.getWindow() ;
          var TABLE_MEM = w.getPeer ( "TABLE_MEM" ) ;
          var udTABLE_MEM = TABLE_MEM.getSelectedUserXml() ;
          var TABLE_OVERVIEW = w.getPeer ( "TABLE_OVERVIEW" ) ;
          var udTABLE_OVERVIEW = TABLE_OVERVIEW.getSelectedUserXml() ;
          var PID = "" ;
          if ( udTABLE_MEM ) PID = udTABLE_MEM.getContent ( "PID" ) ;
          var UID = "" ;
          if ( udTABLE_OVERVIEW ) UID = udTABLE_OVERVIEW.getContent ( "UID" ) ;
          TABLE_MEM.deselectAll() ;
          TABLE_OVERVIEW.deselectAll() ;
          var xml = new TXml() ;
          xml.add ( "NAME", "RefreshMemInfo" ) ;
          var r = null;
          r = TABLE_MEM.findRow ( "PID", PID ) ;
          if ( r ) r.setSelected ( true, true ) ;
          r = TABLE_OVERVIEW.findRow ( "UID", UID ) ;
          if ( r ) r.setSelected ( true, true ) ;
        }'
      />    </Container> 
